{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "security"
  ],
  "rules": {
    "no-eval": "error",
    "eqeqeq": "warn",
    "no-console": "off"
  }
}